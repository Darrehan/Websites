{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": "off",
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "min-height: auto",
          "user-select"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 17.1",
    "not safari <= 17.2"
  ]
}